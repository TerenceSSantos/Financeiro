CREATE TABLE tbl_Endereco
(
   id_endereco integer generated BY DEFAULT AS IDENTITY not null,
   logradouro VARCHAR(255) not null,
   numero VARCHAR(10),
   complemento VARCHAR(50),
   bairro VARCHAR(50),
   cidade VARCHAR(50),
   cep char(8),
   FK_tblEstabelecimento integer,
   FK_tblPessoa integer,
   FK_tblUF char(2),
   CONSTRAINT PK_tblEndereco PRIMARY KEY(id_endereco),
   CONSTRAINT FK_tblEndereco_tblEstabelecimento FOREIGN KEY(FK_tblEstabelecimento) REFERENCES TBL_ESTABELECIMENTO(id_estabelecimento),
   CONSTRAINT FK_tblEndereco_tblPessoa FOREIGN KEY(FK_tblPessoa) REFERENCES TBL_PESSOA(id_pessoa),
   CONSTRAINT FK_tblEndereco_tblUF FOREIGN KEY(FK_tblUF) REFERENCES TBL_UF(sigla_uf)
);
